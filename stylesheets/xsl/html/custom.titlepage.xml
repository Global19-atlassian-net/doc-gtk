<t:templates xmlns:t="http://nwalsh.com/docbook/xsl/template/1.0"
             xmlns:xsl="http://www.w3.org/1999/XSL/Transform">

<t:titlepage element="classset" wrapper="div" class="classset_titlepage">
 <t:titlepage-content side="recto" order="document">
  <xsl:element name="B">
   <classsettitle predicate="[1]"/>
  </xsl:element>
  <desc />
 </t:titlepage-content>
 <t:titlepage-content side="verso">
 </t:titlepage-content>
 <t:titlepage-separator>
     <hr />
 </t:titlepage-separator>
 <t:titlepage-before side="recto" />
 <t:titlepage-before side="verso" />

</t:titlepage>


<t:titlepage element="book" wrapper="div" class="titlepage">
  <t:titlepage-content side="recto">
    <title predicate="[1]"/>
    <subtitle predicate="[1]"/>
    <corpauthor/>
    <authorgroup/>
    <author/>
    <copyright/>
    <legalnotice/>
    <pubdate/>
    <revision/>
    <revhistory/>
    <abstract/>
  </t:titlepage-content>

  <t:titlepage-content side="verso">
  </t:titlepage-content>

  <t:titlepage-separator>
    <hr/>
  </t:titlepage-separator>

  <t:titlepage-before side="recto">
  </t:titlepage-before>

  <t:titlepage-before side="verso">
  </t:titlepage-before>
</t:titlepage>


</t:templates>
