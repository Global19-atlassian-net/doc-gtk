 <glossdiv id="appendix.glossary.s">
  <title>S</title>
  <glossentry id="appendix.glossary.s.signal">
   <glossterm>Signal</glossterm>
   <glossdef>
    <para>
     Notification that is generated whenever an event occurs. May be connected
     to a callback to create signal handlers.
    </para>
    <glossseealso otherterm="appendix.glossary.c.callback" />
    <glossseealso otherterm="appensix.glossary.s.signalhandler" />
   </glossdef>
  </glossentry>
  <glossentry id="appendix.glossary.s.signalhandler">
   <glossterm>Signal Handler</glossterm>
   <glossdef>
    <para>
     A callback that is invoked in response to a signal that is generated.
    </para>
    <glossseealso otherterm="appendix.glossary.c.callback" />
    <glossseealso otherterm="appendix.glossary.s.signal" />
   </glossdef>
  </glossentry>
 </glossdiv>

